        li 
          span.list__left 
            span.text_wr Softcap
          span.lift__right US 
            span.lift__right-colored $3M
        li 
          span.list__left 
            span.text_wr Hardcap
          span.lift__right US 
            span.lift__right-colored $8M
        li 
          span.list__left 
            span.text_wr Token amount
          span.lift__right
            span.lift__right-colored 1,000,000,000
        li 
